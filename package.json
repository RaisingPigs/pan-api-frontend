{
  "name": "pan-api-frontend",
  "version": "4.3.0",
  "description": "admin脚手架",
  "author": {
    "name": "pan",
    "email": "xxxxxxx@qq.com",
    "url": "xxxxxx"
  },
  "repository": {
    "type": "git",
    "url": "xxxxxx"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "run-test": "vite --mode test",
    "run-prod": "vite --mode production",
    "build:test": "vue-tsc --noEmit && vite build --mode test",
    "build:prod": "vue-tsc --noEmit && vite build",
    "preview:test": "pnpm build:test && vite preview",
    "preview:prod": "pnpm build:prod && vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{src,tests,types}/**/*.{vue,js,jsx,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"{src,tests,types}/**/*.{vue,js,jsx,ts,tsx,json,css,less,scss,html,md}\"",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "prepare": "husky install",
    "test": "vitest",

    "build-no-tsc:test": "vite build --mode test",
    "preview-no-tsc:test": "pnpm build-no-tsc:test && vite preview",

    "build-no-tsc:prod": "vite build --mode production",
    "preview-no-tsc:prod": "pnpm build-no-tsc:prod && vite preview"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.3.1",
    "axios": "1.6.5",
    "dayjs": "1.11.10",
    "element-plus": "2.5.2",
    "js-cookie": "3.0.5",
    "lodash-es": "4.17.21",
    "mitt": "3.0.1",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-browserify": "1.0.1",
    "path-to-regexp": "6.2.1",
    "pinia": "2.1.7",
    "screenfull": "6.0.2",
    "vue": "3.4.15",
    "vue-json-pretty": "^2.3.0",
    "vue-router": "4.2.5",
    "vxe-table": "4.4.1",
    "vxe-table-plugin-element": "3.0.7",
    "xe-utils": "3.5.11"
  },
  "devDependencies": {
    "@types/js-cookie": "3.0.6",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.11.5",
    "@types/nprogress": "0.2.3",
    "@types/path-browserify": "1.0.2",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "@vitejs/plugin-vue": "5.0.3",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/test-utils": "2.4.3",
    "eslint": "8.56.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.20.1",
    "husky": "8.0.3",
    "jsdom": "23.2.0",
    "lint-staged": "15.2.0",
    "prettier": "3.2.4",
    "sass": "1.70.0",
    "typescript": "5.3.3",
    "unocss": "0.58.3",
    "vite": "5.0.11",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-svg-loader": "5.1.0",
    "vitest": "1.2.1",
    "vue-eslint-parser": "9.4.0",
    "vue-tsc": "1.8.27"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,scss,html,md}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "keywords": [
    "vue",
    "vue3",
    "admin",
    "vue-admin",
    "vue3-admin",
    "vite",
    "vite-admin",
    "element-plus",
    "element-plus-admin",
    "ts",
    "typescript"
  ],
  "license": "MIT"
}
